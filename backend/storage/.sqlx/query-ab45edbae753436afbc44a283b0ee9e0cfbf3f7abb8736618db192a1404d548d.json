  "query": "\n            SELECT\n                id,\n                title_group_id,\n                name,\n                release_date,\n                release_date_only_year_known,\n                distributor,\n                covers,\n                source AS \"source: _\",\n                additional_information AS \"additional_information: _\",\n                '[]'::jsonb AS \"torrents!: _\"\n            FROM edition_groups\n            WHERE title_group_id = ANY($1)\n            AND (CARDINALITY($2::source_enum[]) = 0 OR source = ANY($2))\n            ",
